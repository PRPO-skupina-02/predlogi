LOG_LEVEL=DEBUG
TZ=

POSTGRES_IP=localhost
POSTGRES_PORT=5432
POSTGRES_USERNAME=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DATABASE_NAME=auth
POSTGRES_TEST_DATABASE_NAME=auth

# External Services (REQUIRED)
AUTH_HOST=http://localhost:8080
NAKUP_HOST=http://localhost:8082
SPORED_HOST=http://localhost:8081

# RabbitMQ (REQUIRED)
RABBITMQ_URL=amqp://guest:guest@localhost:5672/

# OpenRouter Configuration (REQUIRED)
OPENROUTER_API_KEY=sk-or-v1-your-openrouter-key-here
OPENROUTER_MODEL=openai/gpt-4
# Optional: defaults to OpenRouter API if not set
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1
OPENROUTER_MAX_TOKENS=500

# Optional: defaults to 7 days if not set
RECOMMENDATION_LOOKAHEAD_DAYS=7
